@model asi.asicentral.model.Publication
@section featured {
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h1>@ViewBag.Title.</h1>
                <h2>@ViewBag.Message</h2>
            </hgroup>
            <p>
                This Web page shows how to add a specific database item. All other strings are from a Resource file.
            </p>
        </div>
    </section>
}
@using (Html.BeginForm()) {
@Html.AntiForgeryToken()
<h3>@Html.Encode(asi.asicentral.web.Resource.PublicationAddTitle)</h3>  <input type="submit" value="Save" /> @Html.ActionLink("Cancel", "Index");
@Html.HiddenFor(model=>model.PublicationId)
<div class="form-group">
@Html.LabelFor(model => model.Name) @Html.EditorFor(model => model.Name) @Html.ValidationMessageFor(model => model.Name)
</div>
}
@section scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
