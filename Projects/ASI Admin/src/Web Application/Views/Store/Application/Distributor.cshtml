@using asi.asicentral.web
@using asi.asicentral.model.store
@model asi.asicentral.model.store.DistributorMembershipApplication
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Resource.StoreTitleDistributorApplication</h2>

<table class="table table-bordered table-condensed table-striped">
    <tr><td>@Html.LabelFor(m => m.Company)</td><td>@Model.Company</td></tr>
    <tr><td>@Html.LabelFor(m => m.Street1)</td><td>@Model.Street1</td></tr>
    <tr><td>@Html.LabelFor(m => m.Phone)</td><td>@Model.Phone</td></tr>
    <tr><td>@Html.LabelFor(m => m.Fax)</td><td>@Model.Fax</td></tr>
    <tr><td>@Html.LabelFor(m => m.WebUrl)</td><td>@Model.WebUrl</td></tr>
    <tr><td>@Html.LabelFor(m => m.ApplicantName)</td><td>@Model.ApplicantName</td></tr>
    <tr><td>@Html.LabelFor(m => m.ShippingStreet1)</td><td>@Model.ShippingStreet1</td></tr>
    <tr><td>@Html.LabelFor(m => m.NumberOfEmployee)</td><td>@Model.NumberOfEmployee</td></tr>
    <tr><td>@Html.LabelFor(m => m.NumberOfSalesEmployee)</td><td>@Model.NumberOfSalesEmployee</td></tr>
    <tr><td>@Html.LabelFor(m => m.AnnualSalesVolume)</td><td>@Model.AnnualSalesVolume</td></tr>
    <tr><td>@Html.LabelFor(m => m.ASIContact)</td><td>@Model.ASIContact</td></tr>
</table>

<h3>@Resource.DistributorAppTypesOfAccount</h3>
<h3>@Resource.DistributorAppProductLines</h3>
<h3>@Resource.DistributorAgreements</h3>

@section Scripts {
    <script src="/Scripts/asi.js"></script>
}
